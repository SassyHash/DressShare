<%= simple_form_for(@search, :url => search_dresses_path, :method => :get,
:remote => true, :class => 'search-form') do |f| %>

  <%= f.input :brand %>
  <%= f.input :color %>
  <%= f.select :sizes, Dress::SIZES %>
  <%= f.input :start_date, :as => :string, :input_html => {:class => "datepicker"} %>
  <%= f.input :end_date, :input_html => {:class => "datepicker"} %>
  <%= f.submit :search, :name => nil %>
<% end %>

<script type="text/javascript">

$(function() {
    $( ".datepicker" ).datepicker();
    console.log($(".datepicker"));
  });

</script>